{
  "name": "voyager-sst",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sst bind \"vite dev --host\"",
    "build": "vite build",
    "preview": "vite preview",
    "sass-extras": "sass src/theme/extras.scss static/extras.css --load-path=node_modules",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "postinstall": "yarn sass-extras && yarn smui-theme",
    "update": "yarn upgrade --pattern 'scoutradioz*'",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev"
  },
  "devDependencies": {
    "@smui/button": "^7.0.0-beta.0",
    "@smui/card": "^7.0.0-beta.0",
    "@smui/checkbox": "^7.0.0-beta.0",
    "@smui/data-table": "^7.0.0-beta.0",
    "@smui/drawer": "^7.0.0-beta.0",
    "@smui/fab": "^7.0.0-beta.0",
    "@smui/form-field": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@smui/layout-grid": "^7.0.0-beta.0",
    "@smui/linear-progress": "^7.0.0-beta.0",
    "@smui/list": "^7.0.0-beta.0",
    "@smui/paper": "^7.0.0-beta.0",
    "@smui/ripple": "^7.0.0-beta.0",
    "@smui/select": "^7.0.0-beta.0",
    "@smui/slider": "^7.0.0-beta.0",
    "@smui/tab": "^7.0.0-beta.0",
    "@smui/tab-bar": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@smui/top-app-bar": "^7.0.0-beta.0",
    "@smui/touch-target": "^7.0.0-beta.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-cdk-lib": "2.95.1",
    "constructs": "10.2.69",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.69.4",
    "smui-theme": "^7.0.0-beta.15",
    "sst": "^2.29.2",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-kit-sst": "^2.29.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@maslick/koder": "^1.3.2",
    "dexie": "^3.2.4",
    "mongodb": "4.13.0",
    "qrcode": "^1.5.3",
    "scoutradioz-utilities": "../scoutradioz-utilities/",
    "svelte-material-ui": "^7.0.0-beta.15"
  }
}
